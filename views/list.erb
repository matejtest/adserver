
<h1><%= @title %></h1>

<table border="0" cellpadding="0" cellspacing="10" class="list">
	<% @ads.each_with_index do |ad, i| %>
		<tr class="alt">
			<td><%= ad.clicks.size %></td>
			<td><a href="/show/<%= ad.id %>"><%= ad.title %></td>
			<td><a href="/delete/<%= ad.id %>">delete</td>
		</tr>
	<% end %>
</table>